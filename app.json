{
  "expo": {
    "name": "OptionQaaf",
    "slug": "optionqaaf-app",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/white-icon.png",
    "scheme": [
      "optionqaafmobile",
      "shop.85072904499.app"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "bundleIdentifier": "co.shopney.optionqaaf",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We use your location to help you drop a pin on your saved addresses.",
        "NSPhotoLibraryUsageDescription": "We need photo library access so you can attach images to broadcast notifications.",
        "NSUserTrackingUsageDescription": "We do not track users across apps or websites. This permission is requested to comply with App Store requirements."
      }
    },
    "android": {
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "shop.85072904499.app",
              "host": "",
              "path": "/callback"
            }
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_ANDROID_KEY}"
        }
      },
      "package": "co.shopney.optionqaaf"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-notifications",
      "expo-location",
      "expo-asset",
      "expo-localization",
      "@react-native-community/datetimepicker"
    ],
    "notification": {
      "icon": "./assets/images/white-icon.png",
      "color": "#0f172a"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cd3febeb-f2c9-493a-9392-c41e38afad07"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cd3febeb-f2c9-493a-9392-c41e38afad07",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
