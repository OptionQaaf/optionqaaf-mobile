{
  "expo": {
    "name": "OptionQaaf",
    "slug": "optionqaaf-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/white-icon.png",
    "scheme": ["optionqaafmobile", "shop.85072904499.app"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.optionqaaf.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We use your location to help you drop a pin on your saved addresses.",
        "NSPhotoLibraryUsageDescription": "We need photo library access so you can attach images to broadcast notifications."
      }
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "shop.85072904499.app",
              "host": "",
              "path": "/callback"
            }
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_ANDROID_KEY}"
        }
      },
      "package": "com.optionqaaf.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-notifications",
      "expo-location"
    ],
    "notification": {
      "icon": "./assets/images/white-icon.png",
      "color": "#0f172a"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cd3febeb-f2c9-493a-9392-c41e38afad07"
      }
    }
  }
}
